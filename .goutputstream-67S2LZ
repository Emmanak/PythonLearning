#lesson 6 - Looping
print("\n"*10)
print("Lesson 6 - Looping")
for x in range(0,10):
	print(x, ' ', end="")
for y in grocery_list:
	print(y)
for z in [2,4,6,8,10]:
	print(z)
	
num_list = [[1,2,3,], [10,20,30], [100, 200, 300]]

for mat in range (0,3):
	for mat2 in range(0,3):
		print(num_list[mat][mat2])
		
		
#while loops
print("while loops")

while(random_num != 15):
	print(random_num)
	random_num = random.randrange(0,100)
	
	
	

